{
  "name": "@temporaldynamics/ffmpeg-orchestrator",
  "version": "1.1.0",
  "description": "FFmpeg job orchestration engine with deterministic timelines and safe command building",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "temporal-dynamics",
    "ffmpeg",
    "job-queue",
    "media-pipeline",
    "video-render"
  ],
  "author": "Temporal Dynamics LLC",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/temporal-dynamics/neo.git",
    "directory": "librerias/ffmpeg-orchestrator"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.0.0",
    "eslint": "^8.20.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/**/*"
  ]
}
