â”‚    focus-visible:ring-emerald-500",                                     â”‚
â”‚      };                                                                 â”‚
â”‚      return (                                                           â”‚
â”‚        <button type={type} onClick={onClick} className={`${base}        â”‚
â”‚    ${styles[variant]} ${className}`} {...rest}>                         â”‚
â”‚          {children}                                                     â”‚
â”‚        </button>                                                        â”‚
