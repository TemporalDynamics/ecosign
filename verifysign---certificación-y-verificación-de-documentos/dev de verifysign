â”‚    function Backdrop({ onClose }: { onClose?: () => void }) {           â”‚
â”‚      return (                                                           â”‚
â”‚        <motion.div                                                      â”‚
â”‚          aria-hidden                                                    â”‚
â”‚          className="fixed inset-0 z-40 bg-black/50 backdrop-blur-sm"    â”‚
â”‚          onClick={onClose}                                              â”‚
â”‚          initial={{ opacity: 0 }}                                       â”‚
â”‚          animate={{ opacity: 1 }}                                       â”‚
â”‚          exit={{ opacity: 0 }}                                          â”‚
â”‚        />                                                               â”‚
â”‚      );                                                                 â”‚
â”‚    }                                                                    â”‚
â”‚                                                                         â”‚
â”‚    function Card({ children, className = "" }: any) {                   â”‚
â”‚      return (                                                           â”‚
â”‚        <div                                                             â”‚
â”‚          className={                                                    â”‚
â”‚            "rounded-2xl border border-white/10 bg-white/80              â”‚
