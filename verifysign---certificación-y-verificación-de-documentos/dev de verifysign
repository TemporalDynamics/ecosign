â”‚            className                                                    â”‚
â”‚          }                                                              â”‚
â”‚        >                                                                â”‚
â”‚          {children}                                                     â”‚
â”‚        </div>                                                           â”‚
â”‚      );                                                                 â”‚
â”‚    }                                                                    â”‚
â”‚                                                                         â”‚
â”‚    function Button({                                                    â”‚
â”‚      children,                                                          â”‚
â”‚      onClick,                                                           â”‚
â”‚      variant = "primary",                                               â”‚
â”‚      className = "",                                                    â”‚
â”‚      type = "button",                                                   â”‚
â”‚      ...rest                                                            â”‚
â”‚    }: any) {                                                            â”‚
â”‚      const base =                                                       â”‚
â”‚        "inline-flex items-center justify-center gap-2 rounded-xl px-4   â”‚
â”‚    py-2 text-sm font-semibold transition focus:outline-none             â”‚
â”‚    focus-visible:ring-2 focus-visible:ring-offset-2 disabled:opacity-60 â”‚
â”‚     disabled:cursor-not-allowed";                                       â”‚
â”‚      const styles: Record<string, string> = {                           â”‚
â”‚        primary:                                                         â”‚
â”‚          "bg-blue-600 hover:bg-blue-700 text-white                      â”‚
â”‚    focus-visible:ring-blue-500",                                        â”‚
